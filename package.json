{
  "name": "paw",
  "version": "0.1.0",
  "description": "Personal dotfiles manager CLI",
  "type": "module",
  "bin": {
    "paw": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun run build:all",
    "build:all": "bun run build:darwin-arm64 && bun run build:darwin-x64 && bun run build:linux-x64 && bun run build:linux-arm64",
    "build:darwin-arm64": "bun build src/index.ts --compile --target=bun-darwin-arm64 --outfile=dist/paw-darwin-arm64",
    "build:darwin-x64": "bun build src/index.ts --compile --target=bun-darwin-x64 --outfile=dist/paw-darwin-x64",
    "build:linux-x64": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile=dist/paw-linux-x64",
    "build:linux-arm64": "bun build src/index.ts --compile --target=bun-linux-arm64 --outfile=dist/paw-linux-arm64"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.9.2",
    "bun-types": "^1.1.0",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexcatdad/paw.git"
  },
  "author": "Alex Alexandrescu",
  "license": "MIT"
}
